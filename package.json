{
  "name": "studio-coop",
  "private": true,
  "scripts": {
    "prepare": "git config core.hooksPath .githooks",
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "db:migrate": "turbo db:migrate --filter=@studio-coop/db",
    "db:seed": "turbo db:seed --filter=@studio-coop/db",
    "ship": "turbo ship --filter=@studio-coop/api --filter=@studio-coop/web",
    "ship:api": "turbo ship --filter=@studio-coop/api",
    "ship:web": "turbo ship --filter=@studio-coop/web"
  },
  "devDependencies": {
    "turbo": "^2"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  }
}
